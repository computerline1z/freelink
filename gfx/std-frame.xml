<frame label="default">
  <upper-left mode="fixed"><part from="0, 0" to="50%, 50%">button.png</part></upper-left>
  <upper mode="repeat, fixed">titlebar.png</upper>
  <upper-right mode="stretch"><part from="50%, 0" to="100%, 50%">button.png</part></upper-right>
  <left mode="fixed, repeat"><rotate mode="left">titlebar.png</rotate></left>
  <right mode="fixed, repeat"><rotate mode="right">titlebar.png</rotate></right>
  <lower-left mode="stretch"><part from="0, 50%" to="50%, 100%">button.png</part></lower-left>
  <lower mode="repeat, fixed"><rotate mode="180">titlebar.png</rotate></lower>
  <lower-right mode="stretch"><part from="50%, 50%" to="100%, 100%">button.png</part></lower-right>
</frame>
